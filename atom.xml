---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>powery.net</title>
  <link href="{{  site.url   }}"/>
  <link type="application/atom+xml" rel="self" href="{{  site.url  }}/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.url  }}</id>
  <author>
    <name>Eamonn Power</name>
    <email>eamonn@powery.net</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>{{  site.url}}{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://blog.powery.net{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Eamonn Power</name>
      <uri>{{  site.url  }}</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
